{% extends "index.twig" %}

{% block content %}

    {% if page.title %}
        <h2>{{ page.title }}</h2>
    {% endif %}

    <!-- Note that using RAW output format here should be save -->
    <!-- since the text is the result of a request to GitHub's markdown parser -->

    {{ page.text|raw }}

{% endblock %}
