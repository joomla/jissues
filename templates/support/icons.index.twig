{% extends "index.twig" %}

{% block content %}

    <h2>Icons</h2>

    <div class="row">
    {% for icon in icons %}
        <div class="span3 well well-small">
            <i style="font-size: 16px;" class="{{ icon }}"></i>
            &nbsp;{{ icon }}
        </div>
    {% endfor %}
    </div>

{% endblock %}
